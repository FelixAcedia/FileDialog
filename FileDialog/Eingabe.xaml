<Window x:Class="FileDialog.Eingabe"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FileDialog"
        mc:Ignorable="d"
        Title="Eingabe" Height="200" Width="350" WindowStartupLocation="CenterScreen">
    <StackPanel VerticalAlignment="Center" Margin="10">
        <TextBox x:Name="txtBox" Height="50"/>
        <Button Height="35" Width="50" Margin="10" Click="Button_Click">OK</Button>
    </StackPanel>
</Window>
